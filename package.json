{
  "name": "eslint-config-loderunner",
  "version": "4.0.0",
  "description": "Reusable ESLint flat configs for TypeScript, React, Vitest, Vue, and more",
  "type": "module",
  "exports": {
    "./base": "./src/base/config.js",
    "./base/rules": "./src/base/rules.js",
    "./typescript": "./src/typescript/config.js",
    "./typescript/rules": "./src/typescript/rules.js",
    "./import": "./src/import/config.js",
    "./import/rules": "./src/import/rules.js",
    "./react": "./src/react/config.js",
    "./react/rules": "./src/react/rules.js",
    "./vitest": "./src/vitest/config.js",
    "./vitest/rules": "./src/vitest/rules.js",
    "./vue": "./src/vue/config.js",
    "./vue/rules": "./src/vue/rules.js",
    "./jsdoc": "./src/jsdoc/config.js",
    "./jsdoc/rules": "./src/jsdoc/rules.js",
    "./formatting": "./src/formatting/config.js",
    "./formatting/rules": "./src/formatting/rules.js"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "react",
    "vitest",
    "vue",
    "flat-config"
  ],
  "author": "Charles Francoise",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loderunner/eslint-config.git"
  },
  "homepage": "https://github.com/loderunner/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/loderunner/eslint-config/issues"
  },
  "scripts": {
    "lint": "prettier --check . && eslint --max-warnings=0",
    "lint:fix": "prettier --write . && eslint --fix",
    "save-exact-dev-dependencies": "jq -r '.devDependencies | keys[]' package.json | xargs pnpm ls --json | jq -r '.[0].devDependencies[] | \"\\(.from)@\\(.version)\"' | xargs pnpm add --save-dev",
    "prepublishOnly": "pnpm run lint && pnpm run test",
    "test": "vitest run"
  },
  "packageManager": "pnpm@10.9.0",
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@semantic-release/git": "10.0.1",
    "@types/eslint": "9.6.1",
    "@types/node": "24.10.0",
    "@vitest/eslint-plugin": "1.6.5",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "62.0.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-vue": "10.6.2",
    "globals": "17.0.0",
    "prettier": "3.7.4",
    "semantic-release": "25.0.2",
    "testcontainers": "11.11.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "vitest": "2.1.9"
  },
  "peerDependencies": {
    "@eslint/js": "^9.0.0",
    "@vitest/eslint-plugin": "^1.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-import-resolver-typescript": "^4.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^62.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-vue": "^10.6.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-jsdoc": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    }
  }
}
